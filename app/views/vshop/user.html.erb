      <script>
          if ('<%=@user.account.supplier_id %>'!='<%=@supplier.id%>'){
              location.href = "/auto_login?id=<%=@supplier.id%>&<%=request.url.force_encoding('UTF-8')%>&platform=mobile"

          }
      </script>
      <a href="/vshop/<%=@supplier.id%>/user" style="font-size: 20px; "><%=@user.account.id%></a>

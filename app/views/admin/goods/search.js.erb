$("#goods").html('<%=escape_javascript(render @template)%>').attr("data-url",'<%=request.fullpath.html_safe%>');
$(".pagination").html('<%=will_paginate @goods,:container=>false,:params=>{:page=>params[:page]}%>');
$("#goods .order[data-field='<%=@field%>']").addClass("<%=@sorter%>").attr("data-sorter","<%=@next_sorter%>");
$("#goods").attr("count",'<%=@count%>');
$("#goods").attr("goods_ids",'<%=@goods_ids%>');

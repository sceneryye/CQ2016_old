<h4>
  下载详细 : 
  <%if @coupon.brand%>
  [<%=@coupon.brand.brand_name%>]
  <%end%>
  <%=@coupon.name%>
  (<%=@coupon.code%>)
</h4>
<hr>

<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>会员</th>
      <th>下载时间</th>
    </tr>
  </thead>
  <tbody>
  <%@downloads.each do |download|%>
    <tr>
      <td><%=download.user.login_name%></td>
      <td><%=download.downloaded_at.strftime("%Y-%m-%d %H-%M-%S")%></td>
    </tr>
    <%end%>
    </tbody>
  </table>

  <div class="pagination">
    <%=will_paginate @downloads,:container=>false%>
  </div>
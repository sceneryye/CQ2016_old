<div id="keywords" class="clearfix">
	<span class="title">TRADE关键字：</span>
	<ul class="">
		<%@home.keywords.each_with_index do |key,index|%>
			<li>
				<a href="<%=key['link_url']%>"><%=key["word"]%></a>
			</li>
			<%unless @home.keywords.size - 1 == index%>
				<li class="diver">|</li>
			<%end%>
		<%end%>
	</ul>
</div>